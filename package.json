{
  "name": "fullstack-boilerplate-2022",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.development next dev",
    "dev:resetdb": "dotenv -e .env.development -- npx prisma migrate reset --force",
    "dev:migrate": "dotenv -e .env.development -- npx prisma migrate dev --name postgres-init",
    "dev:migrate:only": "dotenv -e .env.development -- npx prisma migrate dev --create-only",
    "test:resetdb": "dotenv -e .env.test -- npx prisma migrate reset --force",
    "test": "yarn test:resetdb && jest",
    "test:watch": "yarn test:resetdb && jest --watch",
    "test:migrate": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "codegen": "graphql-codegen --config codegen.yml",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only graphql/schema",
    "build:nexus": "yarn build:nexus-typegen && next build",
    "graphql:generate": "yarn prisma generate && yarn build:nexus-typegen && yarn codegen",
    "studio:dev": "npx dotenv -e .env.development -- prisma studio",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "sandbox": "dotenv -e .env.development -- ts-node --compiler-options '{\"module\":\"CommonJS\"}' --files"
  },
  "prisma": {
    "seed": "ts-node -r tsconfig-paths/register --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.7.0",
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.10",
    "@prisma/client": "^4.5.0",
    "apollo-server-micro": "^3.10.3",
    "bcrypt": "^5.1.0",
    "framer-motion": "^7.6.1",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.0",
    "lodash.identity": "^3.0.0",
    "lodash.pickby": "^4.6.0",
    "micro": "^9.4.1",
    "micro-cors": "^0.1.1",
    "next": "12.3.1",
    "next-auth": "^4.15.0",
    "nexus": "^1.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.38.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@chakra-ui/storybook-addon": "^4.0.12",
    "@faker-js/faker": "^7.6.0",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/client-preset": "^1.1.0",
    "@graphql-codegen/typescript": "^2.7.5",
    "@graphql-codegen/typescript-operations": "^2.5.5",
    "@graphql-codegen/typescript-react-apollo": "^3.3.5",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/cypress": "^8.0.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.2.0",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "^18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "apollo-server-testing": "3.0.0-alpha.4",
    "babel-loader": "^8.2.5",
    "cypress": "^10.11.0",
    "cypress-social-logins": "^1.14.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.26.0",
    "eslint-config-next": "12.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-storybook": "^0.6.6",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.2",
    "jest-mock-extended": "^3.0.1",
    "prisma": "^4.5.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
